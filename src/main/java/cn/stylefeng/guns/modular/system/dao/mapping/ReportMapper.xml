        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="phone" property="phone" />
        <result column="be_reported_company_name" property="beReportedCompanyName" />
        <result column="be_reported_name" property="beReportedName" />
        <result column="be_reported_address" property="beReportedAddress" />
        <result column="economic_nature" property="economicNature" />
        <result column="prove_information" property="proveInformation" />
